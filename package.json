    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.31.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
